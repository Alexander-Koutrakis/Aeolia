  m_EditorClassIdentifier: 
  DialogueTitle: "\u039A\u03B1\u03C1\u03C5\u03B4\u03B9\u03AC"
  DialogueSprite: {fileID: 21300000, guid: 5c868f9336618a14b8d95e74389fb409, type: 3}
  Sentences:
  - SpeakerName: "\u0394\u03B7\u03BC\u03BF\u03C3\u03B9\u03BF\u03B3\u03C1\u03AC\u03C6\u03BF\u03C2"
    SpeakerSprite: {fileID: 21300000, guid: b60b4ab94db33e64a921c36fe7d0eadb, type: 3}
    Text: "\u03A4\u03BF\u03CD\u03C4\u03BF \u03C4\u03BF \u03B4\u03AD\u03BD\u03C4\u03C1\u03BF
      \u03BC\u03BF\u03B9\u03AC\u03B6\u03B5\u03B9 \u03BD\u03B1 \u03B2\u03C1\u03AF\u03C3\u03BA\u03B5\u03C4\u03B1\u03B9
      \u03B5\u03B4\u03CE \u03B1\u03B9\u03CE\u03BD\u03B5\u03C2."
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u039F\u03B9 \u03BA\u03B1\u03C1\u03C5\u03B4\u03B9\u03AD\u03C2 \u03B6\u03BF\u03C5\u03BD
      \u03C0\u03BF\u03BB\u03CD. \u03A4\u03BF\u03CD\u03C4\u03B7 \u2018\u03B4\u03C9
      \u03C4\u03B7\u03BD \u03C6\u03CD\u03C4\u03B5\u03C8\u03B5 \u03BC\u03CC\u03BD\u03BF
      \u03C4\u03BF\u03C5 \u03AD\u03BD\u03B1 \u03BA\u03BF\u03C1\u03B9\u03C4\u03C3\u03AC\u03BA\u03B9\n\u03C0\u03C1\u03B9\u03BD
      100 \u03C7\u03C1\u03CC\u03BD\u03B9\u03B1. \u0392\u03B9\u03B1\u03B6\u03CC\u03C4\u03B1\u03BD.
      \u0395\u03AF\u03C7\u03B5 \u03C0\u03BF\u03BB\u03CD \u03C0\u03B5\u03C1\u03B9\u03AD\u03C1\u03B3\u03B5\u03B9\u03B1
      \u03BD\u03B1 \u03BC\u03AC\u03B8\u03B5\u03B9 \u03C4\u03BF\u03BD \u03BA\u03CC\u03C3\u03BC\u03BF."
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u039F\u03B9 \u03AC\u03BD\u03B8\u03C1\u03C9\u03C0\u03BF\u03B9 \u03B5\u03B4\u03CE
      \u03BB\u03AD\u03BD\u03B5 \u03C0\u03C9\u03C2 \u03CC\u03C0\u03BF\u03B9\u03BF\u03C2
      \u03C6\u03C5\u03C4\u03AD\u03C8\u03B5\u03B9 \u03BA\u03B1\u03C1\u03C5\u03B4\u03B9\u03AC
      \u03C0\u03B5\u03B8\u03B1\u03AF\u03BD\u03B5\u03B9.\n\u0388\u03C4\u03C3\u03B9
      \u03C0\u03AD\u03B8\u03B1\u03BD\u03B5 \u03BD\u03C9\u03C1\u03AF\u03C2 \u03BA\u03B9
      \u03B1\u03C5\u03C4\u03CC \u03C4\u03BF \u03BA\u03B1\u03BA\u03CC\u03BC\u03BF\u03B9\u03C1\u03BF.
      \u0397 \u03BC\u03BF\u03AF\u03C1\u03B1 \u03B2\u03BB\u03AD\u03C0\u03B5\u03B9\u03C2\u2026"
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u0389\u03C4\u03B1\u03BD \u03BD\u03B1 \u03C4\u03B7\u03BD \u03C6\u03C5\u03C4\u03AD\u03C8\u03B5\u03B9
      \u03BF \u03B3\u03B5\u03C1\u03BF-\u0399\u03C9\u03C3\u03AE\u03C6, \u03C0\u03BF\u03C5
      \u03BC\u03C0\u03CC\u03BB\u03B9\u03B1\u03B6\u03B5 \u03C4\u03B1 \u03B4\u03AD\u03BD\u03C4\u03C1\u03B1.\n\u03A4\u03B1
      \u03B1\u03B3\u03B1\u03C0\u03BF\u03CD\u03C3\u03B5 \u03C4\u03CC\u03C3\u03BF \u03C4\u03B1
      \u03B4\u03AD\u03BD\u03C4\u03C1\u03B1 \u03BF \u03B3\u03AD\u03C1\u03BF\u03C2,
      \u03B6\u03BF\u03CD\u03C3\u03B5 \u03BC\u03B1\u03B6\u03AF \u03C4\u03BF\u03C5\u03C2."
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u0395\u03AF\u03C7\u03B5 \u03BA\u03B9 \u03B1\u03C5\u03C4\u03CC\u03C2 \u03BC\u03B9\u03B1
      \u03B1\u03B3\u03AC\u03C0\u03B7 \u03C3\u03C4\u03BF \u03BD\u03B7\u03C3\u03AF,
      \u03B1\u03BB\u03BB\u03AC \u03BE\u03AD\u03BC\u03B5\u03B9\u03BD\u03B5 \u03B5\u03B4\u03CE.\n\u0394\u03B5\u03BD
      \u03BC\u03C0\u03CC\u03C1\u03B5\u03C3\u03B5 \u03C0\u03BF\u03C4\u03AD \u03BD\u03B1
      \u03B3\u03C5\u03C1\u03AF\u03C3\u03B5\u03B9, \u03BF\u03CD\u03C4\u03B5 \u03AE\u03B8\u03B5\u03BB\u03B5.
      \u039F\u03CD\u03C4\u03B5 \u03C3\u03C4\u03BF\u03BD \u03B4\u03B9\u03C9\u03B3\u03BC\u03CC\u2026"
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u039C\u03C0\u03CC\u03BB\u03B9\u03B1\u03B6\u03B5 \u03AD\u03BD\u03B1 \u03B4\u03AD\u03BD\u03C4\u03C1\u03BF
      \u03B3\u03B9\u03B1 \u03BA\u03AC\u03B8\u03B5 \u03C0\u03B1\u03B9\u03B4\u03AF
      \u03BA\u03B1\u03B9 \u03C4\u03BF \u03AD\u03B2\u03B1\u03B6\u03B5 \u03BD\u03B1
      \u03B1\u03BA\u03BF\u03CD\u03C3\u03B5\u03B9.\n\u039D\u03B1 \u03B1\u03BA\u03BF\u03CD\u03C3\u03B5\u03B9
      \u03C4\u03B7\u03BD \u03BA\u03B1\u03C1\u03B4\u03B9\u03AC \u03C4\u03BF\u03C5
      \u03B4\u03AD\u03BD\u03C4\u03C1\u03BF\u03C5 \u03BA\u03B1\u03B9 \u03BD\u03B1
      \u03C4\u03BF \u03B1\u03B3\u03B1\u03C0\u03AE\u03C3\u03B5\u03B9."
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u039E\u03AD\u03C1\u03B5\u03B9\u03C2 ...\u03BA\u03B1\u03B9 \u03C4\u03B7\u03BD
      \u039A\u03BF\u03BA\u03BA\u03B9\u03BD\u03BF\u03C3\u03BA\u03BF\u03C5\u03C6\u03AF\u03C4\u03C3\u03B1
      \u03C4\u03B1 \u03B4\u03AD\u03BD\u03C4\u03C1\u03B1 \u03C4\u03B7\u03BD \u03AD\u03C3\u03C9\u03C3\u03B1\u03BD.\n\u03A4\u03B7\u03C2
      \u03BC\u03AE\u03BD\u03C5\u03C3\u03B1\u03BD \u03BD\u03B1 \u03C3\u03C9\u03B8\u03B5\u03AF
      \u03B1\u03C0\u2019 \u03C4\u03BF\u03BD \u03BB\u03CD\u03BA\u03BF, \u03C4\u03B7\u03BD
      \u03C0\u03AE\u03C1\u03B1\u03BD \u03BA\u03BF\u03BD\u03C4\u03AC \u03C4\u03BF\u03C5\u03C2."
  - SpeakerName: "\u0393\u03AD\u03C1\u03BF\u03BD\u03C4\u03B1\u03C2"
    SpeakerSprite: {fileID: -2679761648680156155, guid: 0fcfbc099312b474dab42de03c01cb5c, type: 3}
    Text: "\u039A\u03B9 \u03B1\u03C5\u03C4\u03AE \u03AD\u03B3\u03B9\u03BD\u03B5 \u03B5\u03BB\u03AC\u03C6\u03B9.
      \u03A4\u03BF \u03B5\u03BB\u03AC\u03C6\u03B9 \u03C4\u03C9\u03BD \u039A\u03B9\u03BC\u03B9\u03BD\u03C4\u03AD\u03BD\u03B9\u03C9\u03BD.\n\u0388\u03C4\u03C3\u03B9
      \u03BC\u03B1\u03C2 \u03AD\u03BB\u03B5\u03B3\u03B1\u03BD \u03BF\u03B9 \u03B3\u03B9\u03B1\u03B3\u03B9\u03AC\u03B4\u03B5\u03C2
      \u03BC\u03B1\u03C2 \u03C4\u03B7\u03BD \u03B9\u03C3\u03C4\u03BF\u03C1\u03AF\u03B1
      \u03C4\u03B7\u03C2 \u03B5\u03B4\u03CE."
  - SpeakerName: "\u0394\u03B7\u03BC\u03BF\u03C3\u03B9\u03BF\u03B3\u03C1\u03AC\u03C6\u03BF\u03C2"
    SpeakerSprite: {fileID: 21300000, guid: b60b4ab94db33e64a921c36fe7d0eadb, type: 3}
    Text: "\u039D\u03B1 \u2018\u03C7\u03B1\u03BD \u03BA\u03B1\u03B9 \u03C3\u03AE\u03BC\u03B5\u03C1\u03B1
      \u03BF\u03B9 \u03AC\u03BD\u03B8\u03C1\u03C9\u03C0\u03BF\u03B9 \u03C4\u03AD\u03C4\u03BF\u03B9\u03B1
      \u03C3\u03C7\u03AD\u03C3\u03B7 \u03BC\u03B5 \u03C4\u03B7 \u03C6\u03CD\u03C3\u03B7\u2026\n\u03A4\u03B9
      \u03BA\u03B1\u03BB\u03AC \u03C0\u03BF\u03C5 \u03B8\u03B1 \u2018\u03C4\u03B1\u03BD..."
