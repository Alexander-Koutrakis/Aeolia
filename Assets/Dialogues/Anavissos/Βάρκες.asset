  m_Script: {fileID: 11500000, guid: 62476d73ebc701b4484122dea1c697b9, type: 3}
  m_Name: "\u0392\u03AC\u03C1\u03BA\u03B5\u03C2"
  m_EditorClassIdentifier: 
  Sentences:
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u03A3\u03B5 \u03B2\u03BB\u03AD\u03C0\u03C9 \u03C0\u03BF\u03C5 \u03B1\u03B3\u03BD\u03B1\u03BD\u03C4\u03B5\u03CD\u03B5\u03B9\u03C2
      \u03C4\u03B7\u03BD \u03B8\u03AC\u03BB\u03B1\u03C3\u03C3\u03B1 \u03BA\u03B1\u03B9
      \u03C4\u03B9\u03C2 \u03B2\u03AC\u03C1\u03BA\u03B5\u03C2.\n\u03A3\u03B1\u03BD
      \u03BD\u03B1 \u03C0\u03B5\u03C1\u03B9\u03BC\u03AD\u03BD\u03B5\u03B9\u03C2 \u03BA\u03AC\u03C4\u03B9
      \u03BA\u03B1\u03B9 \u03C3\u03B1\u03BD \u03BD\u03B1 \u03B8\u03AD\u03BB\u03B5\u03B9\u03C2
      \u03BD\u03B1 \u03C6\u03CD\u03B3\u03B5\u03B9\u03C2 \u03BC\u03B1\u03B6\u03AF."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u039E\u03AD\u03C1\u03B5\u03B9\u03C2 \u03BF \u03C0\u03B1\u03C0\u03C0\u03BF\u03CD\u03C2
      \u03BC\u03BF\u03C5 \u03B5\u03AF\u03C7\u03B5 \u03BC\u03B9\u03B1 \u03B2\u03AC\u03C1\u03BA\u03B1
      \u03BC\u03B5 \u03C4\u03BF \u03CC\u03BD\u03BF\u03BC\u03B1 \u03C4\u03B7\u03C2
      \u03B3\u03B9\u03B1\u03B3\u03B9\u03AC\u03C2\n\u03C0\u03BF\u03C5 \u03C0\u03BD\u03AF\u03B3\u03B7\u03BA\u03B5
      \u03C3\u03C4\u03B1 \u03BA\u03B1\u03BB\u03CD\u03B2\u03B9\u03B1 \u03C4\u03C9\u03BD
      \u03C0\u03C1\u03BF\u03C3\u03C6\u03CD\u03B3\u03C9\u03BD \u03C3\u03B5 \u03BC\u03B9\u03B1
      \u03BD\u03B5\u03C1\u03BF\u03C0\u03BF\u03BD\u03C4\u03AE."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u03A0\u03AE\u03B3\u03B1\u03B9\u03BD\u03B5 \u03C3\u03C5\u03C7\u03BD\u03AC
      \u03C3\u03C4\u03B7\u03BD \u0391\u03AF\u03B3\u03B9\u03BD\u03B1 \u03BC\u03B5
      \u03B1\u03C5\u03C4\u03AE, \u03BA\u03B1\u03B9 \u03AD\u03BA\u03B1\u03BD\u03B5
      \u03B4\u03BF\u03C5\u03BB\u03B5\u03B9\u03AD\u03C2.\n\u0388\u03C6\u03B5\u03C1\u03BD\u03B5
      \u03BA\u03B1\u03B9 \u03BD\u03AD\u03B1 \u03B3\u03B9\u03B1 \u03C4\u03B1 \u03BA\u03B1\u03C1\u03AC\u03B2\u03B9\u03B1
      \u03C0\u03BF\u03C5 \u03AD\u03C1\u03C7\u03BF\u03BD\u03C4\u03B1\u03B9 \u03C3\u03C4\u03BF\u03BD
      \u03A0\u03B5\u03B9\u03C1\u03B1\u03B9\u03AC."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u03A1\u03C9\u03C4\u03BF\u03CD\u03C3\u03B1\u03BD \u03BF\u03B9 \u03BC\u03AC\u03BD\u03B5\u03C2
      \u03C0\u03BF\u03C5 \u03C0\u03B5\u03C1\u03AF\u03BC\u03B5\u03BD\u03B1\u03BD \u03C4\u03BF\u03C5\u03C2
      \u03B1\u03B9\u03C7\u03BC\u03AC\u03BB\u03C9\u03C4\u03BF\u03C5\u03C2,\n\u03C0\u03BF\u03C5
      \u03B5\u03AF\u03C7\u03B1\u03BD \u03C4\u03B1 \u03C0\u03B1\u03B9\u03B4\u03B9\u03AC
      \u03C4\u03BF\u03C5\u03C2 \u03BF\u03B9 \u03A4\u03BF\u03CD\u03C1\u03BA\u03BF\u03B9
      \u03C3\u03C4\u03B1 \u03C4\u03AC\u03B3\u03BC\u03B1\u03C4\u03B1 \u03B5\u03C1\u03B3\u03B1\u03C3\u03AF\u03B1\u03C2."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u03A0\u03B5\u03C1\u03AF\u03BC\u03B5\u03BD\u03B5 \u03B7 \u03BA\u03C5\u03C1\u03B1-\u039C\u03B1\u03C1\u03AF\u03B1
      \u03C4\u03BF\u03BD \u0386\u03B3\u03B3\u03B5\u03BB\u03BF... \u03A4\u03C5\u03C6\u03BB\u03AE
      \u03BA\u03AC\u03B8\u03B5 \u03B2\u03C1\u03AC\u03B4\u03C5 \u03BA\u03BF\u03AF\u03C4\u03B1\u03B6\u03B5
      \u03C4\u03B7 \u03B8\u03AC\u03BB\u03B1\u03C3\u03C3\u03B1.\n\u039A\u03B1\u03B9
      \u03AD\u03B2\u03B1\u03B6\u03B5 \u03C4\u03BF\u03BD \u0391\u03BD\u03C4\u03C1\u03AD\u03B1
      \u03C0\u03BF\u03C5 \u2018\u03C7\u03B5 \u03B3\u03C5\u03C1\u03AF\u03C3\u03B5\u03B9
      \u03C0\u03C1\u03CE\u03C4\u03BF\u03C2, \u03BD\u03B1 \u03C4\u03B9\u03C2 \u03BB\u03AD\u03B5\u03B9
      \u03B9\u03C3\u03C4\u03BF\u03C1\u03AF\u03B5\u03C2."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u039C\u03CC\u03BD\u03BF \u03C0\u03BF\u03C5 \u03BF \u0391\u03BD\u03C4\u03C1\u03AD\u03B1\u03C2
      \u03B5\u03AF\u03C7\u03B5 \u03B4\u03B5\u03B9 \u03C4\u03BF\u03BD \u0386\u03B3\u03B3\u03B5\u03BB\u03BF
      \u03BD\u03B1 \u03C3\u03BA\u03BF\u03C4\u03CE\u03BD\u03B5\u03C4\u03B1\u03B9.
      \u03A4\u03BF \u03BA\u03C1\u03AC\u03C4\u03B1\u03B3\u03B5.\n\u0394\u03B5\u03BD
      \u03C4\u03BF \u2018\u03C0\u03B5 \u03C0\u03BF\u03C4\u03AD \u03BF\u03CD\u03C4\u03B5
      \u03C3\u03C4\u03B7\u03BD \u03BC\u03AC\u03BD\u03B1 \u03C4\u03BF\u03C5, \u03BF\u03CD\u03C4\u03B5
      \u03C3\u03C4\u03B7\u03BD \u03BA\u03C5\u03C1\u03AC-\u039C\u03B1\u03C1\u03AF\u03B1."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u03A4\u03BF\u03BD \u03AD\u03B2\u03BB\u03B5\u03C0\u03B5 \u03CC\u03BC\u03C9\u03C2
      \u03C4\u03BF\u03BD \u0386\u03B3\u03B3\u03B5\u03BB\u03BF \u03C3\u03C4\u03BF
      \u03CC\u03BD\u03B5\u03B9\u03C1\u03CC \u03C4\u03BF\u03C5. \u039A\u03B9 \u03AD\u03BA\u03B1\u03BD\u03B5
      \u03C7\u03C1\u03CC\u03BD\u03BF \u03C0\u03BF\u03BB\u03CD,\n\u03BD\u03B1 \u03BC\u03C0\u03BF\u03C1\u03AD\u03C3\u03B5\u03B9
      \u03BD\u03B1 \u03C6\u03CD\u03B3\u03B5\u03B9 \u03B7 \u03C3\u03BA\u03B9\u03AC
      \u03B1\u03C0\u03CC \u03C4\u03B1 \u03BC\u03AC\u03C4\u03B9\u03B1 \u03C4\u03BF\u03C5
      \u0391\u03BD\u03C4\u03C1\u03AD\u03B1."
  - SpeakerName: "\u0393\u03B9\u03B1\u03B3\u03B9\u03AC"
    SpeakerSprite: {fileID: 0}
    Text: "\u039C\u03B5\u03C4\u03AC \u03C3\u03BA\u03CC\u03C4\u03C9\u03C3\u03B1\u03BD
      \u03C4\u03B7\u03BD \u03B1\u03B3\u03B1\u03C0\u03B7\u03BC\u03AD\u03BD\u03B7 \u03C4\u03BF\u03C5,
      \u03C4\u03B7\u03BD \u03BA\u03CC\u03C1\u03B7 \u03C4\u03BF\u03C5 \u03B3\u03B9\u03B1\u03C4\u03C1\u03BF\u03CD.\n\u039A\u03B1\u03B9
      \u03C4\u03AF\u03C0\u03BF\u03C4\u03B1 \u03B4\u03B5\u03BD \u03AD\u03BC\u03B5\u03B9\u03BD\u03B5
      \u03C0\u03B9\u03B1 \u03C3\u03C4\u03BF \u03C0\u03B1\u03BB\u03B9\u03BA\u03AC\u03C1\u03B9
      \u03C0\u03B1\u03C1\u03AC \u03BD\u03B1 \u03BE\u03B5\u03BD\u03B9\u03C4\u03B5\u03C5\u03C4\u03B5\u03AF
      \u03C0\u03AC\u03BB\u03B9."
  ScenesToUnlock: []
